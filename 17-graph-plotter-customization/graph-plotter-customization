# Install library matplotlib
# pip3 install matplotlib

import matplotlib.pyplot as plt

# Coordinates for the graph
x = [2, 4, 5, 6]
y = [2, 3, 6, 7]

# Plot the point
plt.plot(x, y, color = 'blue' , linestyle = 'dashed', linewidth = '3', marker = 'o', markerfacecolor = 'red', markersize = 12)

# Select the x and y ranges
plt.ylim(1,8)
plt.xlim(1,8)

plt.xlabel('X Axis')
plt.ylabel('Y Axis')

plt.title('Demo Graph Customization')

plt.show()